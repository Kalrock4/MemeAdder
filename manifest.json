{ 
	"name": "MemeAdder", 
	"version": "1.0",
	"description": "More memes!",
	"browser_action": {   
		"default_icon": "ad-icon.png",
		"default_title": "Memes!"
	},
	"permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"],
	"background": {
      "scripts": ["blocked_domains.js", "background.js"]
    },
    "browser_action": {
    	"default_popup": "popup.html"
    },
	"manifest_version": 2
}

